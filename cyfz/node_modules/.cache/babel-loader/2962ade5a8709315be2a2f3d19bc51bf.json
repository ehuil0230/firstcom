Revenue=res.info.taxRevenue;\r\n                that.stryForm.totalRevenue=res.info.totalRevenue;\r\n                res.info.productList.forEach((item,index)=>{\r\n                    that.$set(that.getprojectDataWay.data,index,item)\r\n                })\r\n            }).catch(function (err) {\r\n                console.log(err)\r\n            })\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .yb-title {\r\n        font-size: 40px;\r\n        padding-left: 400px;\r\n    }\r\n    .yb-form {\r\n        margin-left: 150px;\r\n        margin-top: 30px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/views/mainPage"}]}